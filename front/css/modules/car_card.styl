.cards-wrapper
    min-height 500px
    padding-top 3em

.title 
    text-align center
    display grid
    border 1px solid #efefef
    background-color rgb(181, 181, 181)

.content 
    display grid
    grid-template-columns repeat(6, 1fr)
    gap .2em

    &__head
        text-align center
        border 1px solid #efefef
        background-color #EEE
        padding .5em
        
.row
    display grid
    grid-template-columns repeat(6, 1fr)
    gap .2em

    &__item
        border 1px solid #efefef
        padding .5em
